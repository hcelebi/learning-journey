{
  "consumer": {
    "name": "learning-journey-consumer"
  },
  "provider": {
    "name": "learning-journey-provider"
  },
  "interactions": [
    {
      "description": "Hello world message",
      "providerState": "consumer requests a message from provider",
      "request": {
        "method": "GET",
        "path": "/message"
      },
      "response": {
        "status": 200,
        "headers": {
        },
        "body": {
          "message": "Hello world"
        },
        "matchingRules": {
          "$.body.message": {
            "match": "type"
          }
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    }
  }
}